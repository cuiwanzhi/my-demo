{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      
    ]
  },
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-namespace": false,
    "ordered-imports": false,
    "no-eval": false,
    "variable-name": false,
    "jsdoc-format": false,
    "no-var-requires": false,
    "only-arrow-functions": false,
    "space-before-function-paren": false
    // "max-line-length": [
    //   true,
    //   81
    // ]
  },
  "rulesDirectory": [
    "./src"
  ]
}
